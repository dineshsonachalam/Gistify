{
  "name": "@microlink/mql",
  "description": "Microlink Query Language. The official HTTP client to interact with Microlink API for Node.js, browsers & Deno.",
  "homepage": "https://nicedoc.io/microlinkhq/mql",
  "version": "0.9.3",
  "browser": "src/browser.js",
  "main": "src/node.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microlinkhq/mql.git"
  },
  "bugs": {
    "url": "https://github.com/microlinkhq/mql/issues"
  },
  "keywords": [
    "api",
    "language",
    "microlink",
    "mql",
    "query"
  ],
  "dependencies": {
    "flattie": "~1.1.0",
    "got": "~11.8.2",
    "is-url-http": "~2.2.3",
    "qss": "~2.0.3",
    "whoops": "~4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@rollup/plugin-commonjs": "latest",
    "@rollup/plugin-node-resolve": "latest",
    "abort-controller": "latest",
    "ava": "latest",
    "beauty-error": "latest",
    "chalk": "latest",
    "ci-publish": "latest",
    "conventional-github-releaser": "latest",
    "eachdir": "latest",
    "esm": "latest",
    "exists-file": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "ky": "latest",
    "lint-staged": "latest",
    "meow": "latest",
    "node-fetch": "latest",
    "npm-check-updates": "latest",
    "nyc": "latest",
    "pinst": "latest",
    "prettier-standard": "latest",
    "rollup": "latest",
    "rollup-plugin-filesize": "latest",
    "rollup-plugin-replace": "latest",
    "rollup-plugin-shim": "latest",
    "rollup-plugin-terser": "latest",
    "rollup-plugin-visualizer": "latest",
    "standard": "latest",
    "standard-version": "latest",
    "stream-to-promise": "latest",
    "test-listen": "latest",
    "web-streams-polyfill": "latest"
  },
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "dist",
    "lightweight.js",
    "src"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:ky": "[[ -f src/ky.js ]] || rollup node_modules/ky/index.js --format=umd --name=ky --file=src/ky.js",
    "clean": "rm -rf node_modules",
    "dev": "npm run build -- -w",
    "lint": "standard",
    "_postinstall": "husky install",
    "postpublish": "pinst --enable",
    "postrelease": "npm run release:build && npm run release:tags && npm run release:github && (ci-publish || npm publish --access=public)",
    "prepublishOnly": "pinst --disable",
    "prerelease": "git-dirty && npm run update:check && npm test",
    "pretest": "npm run lint && npm run build:ky",
    "release": "git-authors-cli && git add package.json && standard-version -a",
    "release:build": "npm run build && git add dist/ && git commit -m \"build(no-release): generate bundle\"",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin master",
    "test": "nyc ava --verbose",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "license": "MIT",
  "ava": {
    "files": [
      "test/**/*",
      "!test/browser-globals.js",
      "!test/clients.js"
    ],
    "require": [
      "esm"
    ],
    "timeout": "1m"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js,!*.min.js,": [
      "prettier-standard"
    ]
  },
  "standard": {
    "ignore": [
      "dist",
      "src/ky.js"
    ]
  },
  "umd:main": "dist/mql.js",
  "unpkg": "dist/mql.js"
}
